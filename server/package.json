{
  "name": "server-app",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "clean:build": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage:clean": "rm -rf tests/coverage",
    "test:coverage:collect": "jest --coverage --coverageDirectory=tests/coverage --coverageReporters=text --coverageReporters=html",
    "test:coverage": "npm run test:coverage:clean && npm run test:coverage:collect",
    "clean:all": "npm run clean && npm run test:coverage:clean",
    "test:coverage:target": "jest tests/routes/user.test.ts --coverage --collectCoverageFrom=\"src/dao/commonDao.ts\" --coverageDirectory=tests/coverage --coverageReporters=text --coverageReporters=html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/csrf": "^1.3.2",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@types/pg": "^8.15.4",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "xss": "^1.0.15",
    "xss-clean": "^0.1.4"
  }
}